<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/navigation_graph"
    app:startDestination="@id/fragProperties">

    <fragment
        android:id="@+id/fragLogin"
        android:name="com.example.opulexpropertymanagement.layers.ui.LoginFrag"
        android:label="Login" >
        <argument
            android:name="ReasonForLoginInt"
            app:argType="integer"
            android:defaultValue="0" />
        <action
            android:id="@+id/action_fragLogin_to_fragRegister"
            app:destination="@id/fragRegister" />
    </fragment>
    <fragment
        android:id="@+id/fragRegister"
        android:name="com.example.opulexpropertymanagement.layers.ui.RegisterFrag"
        android:label="Register" >
        <action
            android:id="@+id/action_fragRegister_to_fragLogin"
            app:destination="@id/fragLogin" />
    </fragment>
    <fragment
        android:id="@+id/fragTenantOrLandlord"
        android:name="com.example.opulexpropertymanagement.layers.ui.TenantOrLandlordFrag"
        android:label="Actions" >
        <action
            android:id="@+id/action_fragTenantOrLandlord_to_fragProperties"
            app:destination="@id/fragProperties"
            app:popUpTo="@id/fragTenantOrLandlord" />
    </fragment>
    <fragment
        android:id="@+id/fragProperties"
        android:name="com.example.opulexpropertymanagement.layers.ui.PropertiesFrag"
        android:label="Properties" >
        <action
            android:id="@+id/action_fragProperties_to_fragPropertyAdd"
            app:destination="@id/fragPropertyAdd"
            app:popUpTo="@id/fragProperties" />
        <action
            android:id="@+id/action_fragProperties_to_fragPropertyDetails"
            app:destination="@id/fragPropertyDetails"
            app:popUpTo="@id/fragProperties" />
        <action
            android:id="@+id/action_fragProperties_to_fragTenantOrLandlord"
            app:destination="@id/fragTenantOrLandlord" />
    </fragment>
    <action
        android:id="@+id/action_global_fragProperties"
        app:destination="@id/fragProperties" />
    <fragment
        android:id="@+id/fragPropertyAdd"
        android:name="com.example.opulexpropertymanagement.layers.ui.PropertyAddFrag"
        android:label="Add Property" >
        <action
            android:id="@+id/action_fragPropertyAdd_to_fragLogin"
            app:destination="@id/fragLogin"
            app:popUpTo="@id/fragPropertyAdd" />
    </fragment>
    <action
        android:id="@+id/action_global_fragLogin"
        app:destination="@id/fragLogin"
        app:popUpTo="@id/fragProperties" />
    <fragment
        android:id="@+id/fragPropertyDetails"
        android:name="com.example.opulexpropertymanagement.layers.ui.PropertyDetailsFrag"
        android:label="Property Details" >
        <argument
            android:name="property"
            app:argType="com.example.opulexpropertymanagement.models.Property" />
        <action
            android:id="@+id/action_fragPropertyDetails_to_tenantAddFrag"
            app:destination="@id/tenantAddFrag"
            app:popUpTo="@id/fragPropertyDetails" />
        <action
            android:id="@+id/action_fragPropertyDetails_to_tenantDetailsFrag"
            app:destination="@id/tenantDetailsFrag" />
        <action
            android:id="@+id/action_fragPropertyDetails_to_maintenancesFrag"
            app:destination="@id/maintenancesFrag"
            app:popUpTo="@id/fragPropertyDetails" />
    </fragment>
    <fragment
        android:id="@+id/tenantAddFrag"
        android:name="com.example.opulexpropertymanagement.layers.ui.TenantAddFrag"
        android:label="Add Tenant" >
        <argument
            android:name="property"
            app:argType="com.example.opulexpropertymanagement.models.Property" />
    </fragment>
    <fragment
        android:id="@+id/tenantDetailsFrag"
        android:name="com.example.opulexpropertymanagement.layers.ui.TenantDetailsFrag"
        android:label="Tenant" >
        <action
            android:id="@+id/action_tenantDetailsFrag_to_documentDetailsFrag"
            app:destination="@id/documentDetailsFrag"
            app:popUpTo="@id/tenantDetailsFrag" />
        <argument
            android:name="tenant"
            app:argType="com.example.opulexpropertymanagement.models.Tenant" />
    </fragment>
    <fragment
        android:id="@+id/documentDetailsFrag"
        android:name="com.example.opulexpropertymanagement.layers.ui.DocumentDetailsFrag"
        android:label="Document" >
        <argument
            android:name="document"
            app:argType="com.example.opulexpropertymanagement.models.Document" />
    </fragment>
    <fragment
        android:id="@+id/tenantsFrag"
        android:name="com.example.opulexpropertymanagement.layers.ui.TenantsFrag"
        android:label="Tenants" >
        <action
            android:id="@+id/action_tenantsFrag_to_tenantDetailsFrag"
            app:destination="@id/tenantDetailsFrag"
            app:popUpTo="@id/tenantsFrag" />
    </fragment>
    <action
        android:id="@+id/action_global_tenantsFrag"
        app:destination="@id/tenantsFrag"
        app:popUpTo="@id/fragProperties" />
    <fragment
        android:id="@+id/maintenancesFrag"
        android:name="com.example.opulexpropertymanagement.layers.ui.MaintenancesFrag"
        android:label="Maintenances" >
        <action
            android:id="@+id/action_maintenancesFrag_to_maintenanceDetailsFrag"
            app:destination="@id/maintenanceDetailsFrag"
            app:popUpTo="@id/maintenancesFrag" />
        <argument
            android:name="property"
            app:argType="com.example.opulexpropertymanagement.models.Property" />
    </fragment>
    <fragment
        android:id="@+id/maintenanceDetailsFrag"
        android:name="com.example.opulexpropertymanagement.layers.ui.MaintenanceDetailsFrag"
        android:label="Maintenance Details" />
</navigation>